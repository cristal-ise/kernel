<xml-bindings
    xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/oxm" version="2.3"
    package-name="org.cristalise.kernel.lookup"
    xml-accessor-type="NONE">
    <java-types>
        <java-type name="Path">
            <java-attributes>
                <xml-attribute java-attribute="mPath" name="path" type="java.lang.String">
                    <xml-access-methods get-method="getStringPath" set-method="setPath"/>
                </xml-attribute>
            </java-attributes>
        </java-type>

        <java-type name="ItemPath">
            <xml-root-element name="ItemPath"/>
            <java-attributes>
                <xml-value java-attribute="mIOR" type="java.lang.String">
                    <xml-access-methods get-method="getIORString" set-method="setIORString"/>
                </xml-value>
            </java-attributes>
        </java-type>

        <java-type name="AgentPath">
            <xml-root-element name="AgentPath"/>
            <java-attributes>
                <xml-attribute java-attribute="mAgentName" name="agentName" type="java.lang.String">
                    <xml-access-methods get-method="getAgentName" set-method="setAgentName"/>
                </xml-attribute>
            </java-attributes>
        </java-type>

        <java-type name="DomainPath">
            <xml-root-element name="DomainPath"/>
            <java-attributes>
                <xml-value java-attribute="target" type="java.lang.String">
                    <xml-access-methods get-method="getTargetUUID" set-method="setTargetUUID"/>
                </xml-value>
            </java-attributes>
        </java-type>

        <java-type name="RolePath">
            <xml-root-element name="RolePath"/>
            <java-attributes>
                <xml-attribute java-attribute="hasJobList" name="hasJobList" type="boolean">
                    <xml-access-methods get-method="hasJobList" set-method="setHasJobList"/>
                </xml-attribute>
            </java-attributes>
        </java-type>
    </java-types>
</xml-bindings>
